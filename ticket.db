CREATE TABLE movies.ticket (
  id VARCHAR(20) NOT NULL,
  movieName VARCHAR(45) NULL,
  theater VARCHAR(45) NULL,
  date VARCHAR(45) NULL,
  time VARCHAR(45) NULL,
  seats VARCHAR(255) NULL,
  INDEX id_idx (id ASC) VISIBLE,
  CONSTRAINT id
    FOREIGN KEY (id)
    REFERENCES movies.user (id)
    ON DELETE CASCADE
    ON UPDATE CASCADE);
